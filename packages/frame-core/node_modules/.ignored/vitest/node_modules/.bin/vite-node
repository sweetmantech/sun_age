#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/suhayb_pc/Desktop/SUNAGE/sirsu.eth-sun-spin-age/miniapps/node_modules/.pnpm/vite-node@2.1.9_@types+node@22.15.20_lightningcss@1.29.3_terser@5.39.2/node_modules/vite-node/node_modules:/Users/suhayb_pc/Desktop/SUNAGE/sirsu.eth-sun-spin-age/miniapps/node_modules/.pnpm/vite-node@2.1.9_@types+node@22.15.20_lightningcss@1.29.3_terser@5.39.2/node_modules:/Users/suhayb_pc/Desktop/SUNAGE/sirsu.eth-sun-spin-age/miniapps/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/suhayb_pc/Desktop/SUNAGE/sirsu.eth-sun-spin-age/miniapps/node_modules/.pnpm/vite-node@2.1.9_@types+node@22.15.20_lightningcss@1.29.3_terser@5.39.2/node_modules/vite-node/node_modules:/Users/suhayb_pc/Desktop/SUNAGE/sirsu.eth-sun-spin-age/miniapps/node_modules/.pnpm/vite-node@2.1.9_@types+node@22.15.20_lightningcss@1.29.3_terser@5.39.2/node_modules:/Users/suhayb_pc/Desktop/SUNAGE/sirsu.eth-sun-spin-age/miniapps/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vite-node@2.1.9_@types+node@22.15.20_lightningcss@1.29.3_terser@5.39.2/node_modules/vite-node/vite-node.mjs" "$@"
else
  exec node  "$basedir/../../../../../vite-node@2.1.9_@types+node@22.15.20_lightningcss@1.29.3_terser@5.39.2/node_modules/vite-node/vite-node.mjs" "$@"
fi
