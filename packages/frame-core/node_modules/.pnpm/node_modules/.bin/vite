#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/suhayb_pc/Desktop/SUNAGE/sirsu.eth-sun-spin-age/packages/frame-core/node_modules/.pnpm/vite@5.4.19_@types+node@22.15.29/node_modules/vite/bin/node_modules:/Users/suhayb_pc/Desktop/SUNAGE/sirsu.eth-sun-spin-age/packages/frame-core/node_modules/.pnpm/vite@5.4.19_@types+node@22.15.29/node_modules/vite/node_modules:/Users/suhayb_pc/Desktop/SUNAGE/sirsu.eth-sun-spin-age/packages/frame-core/node_modules/.pnpm/vite@5.4.19_@types+node@22.15.29/node_modules:/Users/suhayb_pc/Desktop/SUNAGE/sirsu.eth-sun-spin-age/packages/frame-core/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/suhayb_pc/Desktop/SUNAGE/sirsu.eth-sun-spin-age/packages/frame-core/node_modules/.pnpm/vite@5.4.19_@types+node@22.15.29/node_modules/vite/bin/node_modules:/Users/suhayb_pc/Desktop/SUNAGE/sirsu.eth-sun-spin-age/packages/frame-core/node_modules/.pnpm/vite@5.4.19_@types+node@22.15.29/node_modules/vite/node_modules:/Users/suhayb_pc/Desktop/SUNAGE/sirsu.eth-sun-spin-age/packages/frame-core/node_modules/.pnpm/vite@5.4.19_@types+node@22.15.29/node_modules:/Users/suhayb_pc/Desktop/SUNAGE/sirsu.eth-sun-spin-age/packages/frame-core/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
